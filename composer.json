{
    "name": "woub-chat/doc",
    "description": "Php doc block generator",
    "keywords": [
        "laravel",
        "doc",
        "block"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "woub-chat/object": "*",
        "woub-chat/text": "*",
        "woub-chat/entity": "^1.1.5"
    },
    "require-dev": {

    },
    "autoload": {
        "psr-4": {
            "Bfg\\Doc\\": "src"
        },
        "files": [
            "helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {

        }
    },
    "scripts": {

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bfg\\Doc\\ServiceProvider"
            ],
            "aliases": {
                "Doc": "Bfg\\Doc\\Facade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
